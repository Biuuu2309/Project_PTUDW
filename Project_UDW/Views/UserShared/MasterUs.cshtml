    <style>
        .offcanvas {
            z-index: 1051; /* Higher than .modal-backdrop */
        }

        .modal-backdrop {
            z-index: 1049; /* Lower than .offcanvas */
        }

        .CTNNK {
            background-color: #212529;
            display: flex;
        }

    </style>

</head>
<body>
@{
    await Html.RenderPartialAsync("~/Views/UserShared/HeaderUs.cshtml");
}

@RenderBody()
@{
   await Html.RenderPartialAsync("~/Views/UserShared/FooterUs.cshtml");
}
    <script src="~/bundles/jquery"></script>
    <script src="~/bundles/bootstrap"></script>
    @RenderSection("scripts", required: false)
